     			<button id="btn_launch_pic1" type="button" class="btn btn-default cancel" style="width:70px; height:35px;" data-toggle="modal" onclick="launchPic('normal')">图形 </button> 
    		</div>
    		<div class="col-sm-2">
     			<div style="width:154px; height:100px; text-align:center;">
     				<img src="../../images/clipboard-2.png" style="width:60px; height:70px; margin-top:15px;" />
     			</div>
     			<div style="height:35px;"><label style="width:154px;text-align:center;font-size:18px;">特殊事项审批</label></div>
     			<button id="btn_launch_flow2" type="button" class="btn btn-primary search" style="width:70px; height:35px;" data-toggle="modal" onclick="start('spMatterModal', '特殊事项审批')">发起 </button>
     			<button id="btn_launch_pic2" type="button" class="btn btn-default cancel" style="width:70px; height:35px;" data-toggle="modal" onclick="launchPic('spMatter')">图形 </button> 
    		</div>
    		<div class="col-sm-2">
     			<div style="width:154px; height:100px; text-align:center;">
     				<img src="../../images/car.png" style="width:60px; height:70px; margin-top:15px;" />
     			</div>
     			<div style="height:35px;"><label style="width:154px;text-align:center;font-size:18px;">融资直通车</label></div>
     			<button id="btn_launch_flow2" type="button" class="btn btn-primary search" style="width:70px; height:35px;" data-toggle="modal" onclick="start('financeModal', '融资直通车')">发起 </button>
     			<button id="btn_launch_pic2" type="button" class="btn btn-default cancel" style="width:70px; height:35px;" data-toggle="modal" onclick="launchPic('finance')">图形 </button> 
    		</div>
    		<div class="col-sm-2">
     			<div style="width:154px; height:100px; text-align:center;">
     				<img src="../../images/quick.png" style="width:60px; height:70px; margin-top:15px;" />
     			</div>
     			<div style="height:35px;"><label style="width:154px;text-align:center;font-size:18px;">特殊事项快速审批</label></div>
     			<button id="btn_launch_flow2" type="button" class="btn btn-primary search" style="width:70px; height:35px;" data-toggle="modal" onclick="start('spMatterModal', '特殊事项快速审批')">发起 </button>
     			<button id="btn_launch_pic2" type="button" class="btn btn-default cancel" style="width:70px; height:35px;" data-toggle="modal" onclick="launchPic('spMatterFast')">图形 </button> 
    		</div>
    		    		<div class="col-sm-2" >
     			<div style="width:144px; height:100px; text-align:center;">
     				<img src="../../images/clipboard.png" style="width:60px; height:70px; margin-top:15px;" />
     			</div>
     			<div style="height:35px;"><label style="width:140px;text-align:center;font-size:18px;">在线签约流程</label></div>
     			<button id="btn_launch_flow3" type="button" class="btn btn-primary search" style="width:70px; height:35px;" data-toggle="modal" onclick="start('onlineModal','在线签约')">发起 </button>
     			<button id="btn_launch_pic3" type="button" class="btn btn-default cancel" style="width:70px; height:35px;" data-toggle="modal" onclick="launchPic('online')">图形 </button> 
    		</div>
        </div>
        </div>
        <!-- 图形 -->
        <div class="modal fade" id="picModal" tabindex="-1" role="dialog" aria-labelledby="picModalLabel">
		    <div class="modal-dialog" role="document" style="width:1000px;">
		      <div class="modal-content">
		        <div class="modal-header">
		          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span> </button> 
		          <h4 class="modal-title" id="picModalLabel">标准流程</h4>
		        </div>
		        <div class="modal-body">
		        	<img id="pic" src="?" style="width:100%;" />
		        </div>
	      	  </div>
	    	</div>
	 	</div>
        <!-- 标准流程 -->
        <form class="form-horizontal" role="form" id="normalForm" data-toggle="validator">
	        <div class="modal fade" id="normalModal" tabindex="-1" role="dialog" aria-labelledby="normalModalLabel">
		    <div class="modal-dialog" role="document">
		      <div class="modal-content">
		        <div class="modal-header">
		          <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span> </button> 
		          <h4 class="modal-title" id="normalModalLabel">立项管理</h4>
		        </div>
		        <div class="modal-body">
		        <input type="hidden" id="workflowNm" name="workflowNm" value="">
		          <div class="form-group">
		            <label class="col-sm-4 control-label" for="proName"><span class="required">*</span>项目名称</label>
		            <div class="col-sm-6">
		            	<input type="text" name="proName" class="form-control" id="proName" placeholder="项目名称" maxlength="30">
		            </div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="chaseFlg"><span class="required">*</span>是否有追</label>
			        <div class="col-sm-6">
				        <label class="col-sm-6 control-label textLeft">
							<input type="radio" name="chaseFlg" value="1" checked> 是
						</label>
						<label class="col-sm-6 control-label textLeft">
							<input type="radio" name="chaseFlg" value="0"> 否
						</label>
					</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="factorType"><span class="required">*</span>保理类型</label>
		            <div class="col-sm-6">
		            	<select id="factorType" name="factorType" class="form-control">
		                	<option value="1">明保</option>
		                	<option value="0">暗保</option>
		                </select>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="proMakeDate"><span class="required">*</span>发起日期</label>
		            <div id="divDate" class="col-sm-6 has-feedback">
		            	<input type="text" name="proMakeDate" style="cursor: pointer;" class="form-control" id="proMakeDate" readonly="readonly">
		            	<span class="glyphicon glyphicon-calendar form-control-feedback"></span>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="relBuyName"><span class="required">*</span>关联买方名称</label>
		            <div class="col-sm-6">
		            	<input type="text" name="relBuyName" class="form-control" id="relBuyName" onkeyup="chgRelBuyAndSaleName(this.value, '4', 'normalForm', 'relBuyId')" placeholder="关联买方名称" maxlength="32">
		            	<input type="hidden" name="relBuyId" id="relBuyId">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="relSaleName"><span class="required">*</span>关联卖方名称</label>
		            <div class="col-sm-6">
		            	<input type="text" name="relSaleName" class="form-control" id="relSaleName" onkeyup="chgRelBuyAndSaleName(this.value, '5', 'normalForm', 'relSaleId')" placeholder="关联卖方名称" maxlength="32">
		            	<input type="hidden" name="relSaleId" id="relSaleId">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="relSaleCorpName"><span class="required">*</span>关联企业名称</label>
		            <div class="col-sm-6">
		            	<input type="text" name="relSaleCorpName" class="form-control" id="relSaleCorpName" placeholder="关联企业名称" maxlength="32">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="note"><span class="required">*</span>备注</label>
		            <div class="col-sm-6">
		            	<input type="text" name="note" class="form-control" id="note" placeholder="备注" maxlength="500">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="contract">合同</label>
		            <div class="col-sm-6">
		            	<div class="input-group">
		            		<input type="text" class="form-control" id="contract" name="contract" placeholder="上传附件" readonly>
		            		<span class="input-group-addon">
								<i class="fa fa-ellipsis-h" aria-hidden="true"  onclick="FlowMngCommon.fileSelect(this);"></i>
								<input type="file" name="file" id="contractFile" onchange="FlowMngCommon.ajaxFileUpload(this);" style="display:none;">
					      	</span>
		            	</div>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="order"><span class="required">*</span>订单</label>
		            <div class="col-sm-6">
		            	<div class="input-group" id="divFileUpload">
		            		<input type="text" class="form-control" id="order" name="order" placeholder="上传附件" readonly>
		            		<span class="input-group-addon">
								<i class="fa fa-ellipsis-h" aria-hidden="true"  onclick="FlowMngCommon.fileSelect(this);"></i>
								<input type="file" name="file" id="orderFile" onchange="FlowMngCommon.ajaxFileUpload(this);" style="display:none;">
					      	</span>
		            	</div>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="delivery"><span class="required">*</span>发货单</label>
		            <div class="col-sm-6">
		            	<div class="input-group" id="divFileUpload">
		            		<input type="text" class="form-control" id="delivery" name="delivery" placeholder="上传附件" readonly>
		            		<span class="input-group-addon">
								<i class="fa fa-ellipsis-h" aria-hidden="true"  onclick="FlowMngCommon.fileSelect(this);"></i>
								<input type="file" name="file" id="deliveryFile" onchange="FlowMngCommon.ajaxFileUpload(this);" style="display:none;">
					      	</span>
		            	</div>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="receipt"><span class="required">*</span>收货单</label>
		            <div class="col-sm-6">
		            	<div class="input-group" id="divFileUpload">
		            		<input type="text" class="form-control" id="receipt" name="receipt" placeholder="上传附件" readonly>
		            		<span class="input-group-addon">
								<i class="fa fa-ellipsis-h" aria-hidden="true"  onclick="FlowMngCommon.fileSelect(this);"></i>
								<input type="file" name="file" id="receiptFile" onchange="FlowMngCommon.ajaxFileUpload(this);" style="display:none;">
					      	</span>
		            	</div>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="godownEntry"><span class="required">*</span>入库单</label>
		            <div class="col-sm-6">
		            	<div class="input-group" id="divFileUpload">
		            		<input type="text" class="form-control" id="godownEntry" name="godownEntry" placeholder="上传附件" readonly>
		            		<span class="input-group-addon">
								<i class="fa fa-ellipsis-h" aria-hidden="true"  onclick="FlowMngCommon.fileSelect(this);"></i>
								<input type="file" name="file" id="godownEntryFile" onchange="FlowMngCommon.ajaxFileUpload(this);" style="display:none;">
					      	</span>
		            	</div>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="list"><span class="required">*</span>榜单(保险)</label>
		            <div class="col-sm-6">
		            	<div class="input-group" id="divFileUpload">
		            		<input type="text" class="form-control" id="list" name="list" placeholder="上传附件" readonly>
		            		<span class="input-group-addon">
								<i class="fa fa-ellipsis-h" aria-hidden="true"  onclick="FlowMngCommon.fileSelect(this);"></i>
								<input type="file" name="file" id="listFile" onchange="FlowMngCommon.ajaxFileUpload(this);" style="display:none;">
					      	</span>
		            	</div>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="storage"><span class="required">*</span>仓储单</label>
		            <div class="col-sm-6">
		            	<div class="input-group" id="divFileUpload">
		            		<input type="text" class="form-control" id="storage" name="storage" placeholder="上传附件" readonly>
		            		<span class="input-group-addon">
								<i class="fa fa-ellipsis-h" aria-hidden="true"  onclick="FlowMngCommon.fileSelect(this);"></i>
								<input type="file" name="file" id="storageFile" onchange="FlowMngCommon.ajaxFileUpload(this);" style="display:none;">
					      	</span>
		            	</div>
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="billNo"><span class="required">*</span>合同/订单编号</label>
		            <div class="col-sm-6">
		            	<input type="text" name="billNo" class="form-control" id="billNo" placeholder="合同/订单编号" maxlength="30">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="billName"><span class="required">*</span>合同名称/订单</label>
		            <div class="col-sm-6">
		            	<input type="text" name="billName" class="form-control" id="billName" placeholder="合同名称/订单" maxlength="30">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="billAmount"><span class="required">*</span>合同/订单金额</label>
		            <div class="col-sm-6">
		            	<input type="text" name="billAmount" class="form-control" id="billAmount" placeholder="合同/订单金额">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="payType"><span class="required">*</span>付款方式</label>
		            <div class="col-sm-6">
		            	<input type="text" name="payType" class="form-control" id="payType" placeholder="付款方式" maxlength="20">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="accountPeriod"><span class="required">*</span>合同/订单约定账期</label>
		            <div class="col-sm-6">
		            	<input type="text" name="accountPeriod" class="form-control" id="accountPeriod" placeholder="合同/订单约定账期" maxlength="8">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="hasInv"><span class="required">*</span>是否开立发票</label>
			        <div class="col-sm-6">
				        <label class="radio-inline" style="margin-top:10px; margin-bottom:10px;">
							<input type="radio" name="hasInv" value="1" class="rad" checked> 是
						</label>
						<label class="radio-inline" style="margin-left:110px;">
							<input type="radio" name="hasInv" value="0" class="rad"> 否
						</label>
					</div>
		          </div>
		          <div id="otherInv" class="collapse">
			          
		          </div>
		          <div id="otherInvHtml" style="display:none">
		          	<div class="form-group">
			            <label class="col-sm-4 control-label"></label>
			            <div class="col-sm-6">
			            	<div style="float:left; height:40px;">
			            		<input type="button" value="+"
			            		style="width:40px; height:40px; background-color:#75f25c; font-size:30px; border: none; color: #fff;" onclick="addInv('normalForm')">
			            	</div>
			            	<div style="float:left; margin-left:10px; height:40px; line-height:40px;">添加发票</div>
			            	<div style="float:left; height:40px; margin-left:20px;">
			            		<input type="button" value="-"
			            		style="width:40px; height:40px; background-color:#75f25c; font-size:40px; border: none; color: #fff;" onclick="delInv('normalForm')">
			            	</div>
			            	<div style="float:left; margin-left:10px; height:40px; line-height:40px;">删除发票</div>
			         	</div>
			          </div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="arBal"><span class="required">*</span>应收账款余额</label>
		            <div class="col-sm-6">
		            	<input type="text" name="arBal" class="form-control" id="arBal" placeholder="应收账款余额">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="aplFacAmt"><span class="required">*</span>拟申请保理金额</label>
		            <div class="col-sm-6">
		            	<input type="text" name="aplFacAmt" class="form-control" id="aplFacAmt" placeholder="拟申请保理金额">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="aplPeriod"><span class="required">*</span>申请账期</label>
		            <div class="col-sm-6">
		            	<input type="text" name="aplPeriod" class="form-control" id="aplPeriod" placeholder="申请账期" maxlength="8">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="arPct"><span class="required">*</span>应收账款受让比例</label>
		            <div class="col-sm-6">
		            	<input type="text" name="arPct" class="form-control" id="arPct" placeholder="应收账款受让比例" maxlength="8">
		         	</div>
		          </div>
		          <div class="form-group">
		            <label class="col-sm-4 control-label"  for="srchPrtSc"><span class="required">*</span>中登网查询截屏</label>
		            <div class="col-sm-6">
		            	<div class="input-group" id="divFileUpload">
		            		<input type="text" class="form-control" id="srchPrtSc" name="srchPrtSc" placeholder="上传附件" readonly>
		            		<span class="input-group-addon">
								<i class="fa fa-ellipsis-h" aria-hidden="true"  onclick="FlowMngCommon.fileSelect(this);"></i>
								<input type="file" name="file" id="srchPrtScFile" onchange="FlowMngCommon.ajaxFileUpload(this);" style="display:none;">
					      	</span>
		            	</div>
		         	</div>
		          </div>
		       </div>
		       <div class="modal-footer">
				  <div class="form-group">
				  	<div class="col-sm-4"></div>
					<div class="col-sm-3">
						<button type="button" class="btn btn-default cancel"
							data-dismiss="modal">取消
						</button>
					</div>
					<div class="col-sm-3">
						<button type="submit" class="btn btn-default add"
							onclick="create('normalForm', 'normalModal')">创建
						</button>
					</div>
				</div>
			</div>
	      </div>
	    </div>
	  	</div>
  		</form>
        <!-- 特殊事项审批 -->
        <form class="form-horizontal" role="form" id="spMatterForm" data-toggle="validator">
	        <div class="modal fade" id="spMatterModal" tabindex="-1" role="dialog" aria-labelledby="spMatterModalLabel">
